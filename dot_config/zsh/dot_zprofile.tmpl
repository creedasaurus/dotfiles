#!/usr/bin/env zsh

# ~/.config/zsh/.zprofile

{{ output "brew" "shellenv" | trim }}

path=(
  $HOME/.toolbox/bin
  $HOME/.local/bin
  $HOME/.rodar/bin
  $HOMEBREW_PREFIX/opt/asdf/libexec/bin
  $HOMEBREW_PREFIX/bin
  $path
)

if [ -e $HOME/.nix-profile/etc/profile.d/hm-session-vars.sh ]; then
  . $HOME/.nix-profile/etc/profile.d/hm-session-vars.sh
fi
